over {\n      color: $main-button-color;\n    }\n\n    &--active::after {\n      position: absolute;\n      right: 0;\n      left: 0;\n      bottom: 0;\n      content: \"\";\n      height: 4px;\n      border-radius: 8px;\n      background-color: hsl(193, 100%, 43%);\n    }\n  }\n}\n","styles\\utilits\\_variables.scss":"$main-button-color: #00acdc;\n$card-width: 200px;\n$card-gap: 48px;\n","styles\\blocks\\container.scss":".container {\n  display: flex;\n  justify-content: center;\n\n  &__content {\n    margin: 50px 20px 0;\n  }\n}\n","styles\\blocks\\content.scss":".content {\n  display: grid;\n  grid-template-columns: repeat(4, minmax(200px, 1fr));\n  gap: 50px $card-gap;\n\n  @media (max-width: 984px) {\n    grid-template-columns: repeat(3, minmax(200px, 1fr));\n  }\n\n  @media (max-width: 736px) {\n    grid-template-columns: repeat(2, minmax(200px, 1fr));\n  }\n}\n","styles\\blocks\\card.scss":".card {\n  max-width: $card-width;\n  padding: 14px;\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  border: 1px solid #f3f3f3;\n  border-radius: 5px;\n  perspective: 100px;\n\n  &__image {\n    width: 160px;\n    height: 134px;\n    margin-bottom: 45px;\n    margin-top: 15px;\n    align-self: center;\n    transition: 0.4s;\n\n    &:hover {\n      transform: translate3d(0, 10px, 20px);\n      border: 3px solid #eee;\n    }\n  }\n\n  &__product {\n\n    &-name {\n      font-weight: 500;\n      line-height: 15px;\n      font-size: 12px;\n      margin-bottom: 6px;\n    }\n    \n    &-code {\n      margin-bottom: 15px;\n    }\n  }\n\n  &__rate {\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n    margin-bottom: 20px;\n  }\n\n  &__price {\n    display: flex;\n    justify-content: space-between;\n    align-items: baseline;\n    margin-bottom: 20px;\n  \n    &-value {\n      font-size: 16px;\n      font-weight: 700;\n    }\n  }\n  \n  &__button {\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    height: 40px;\n    background-color: $main-button-color;\n    border: 1px solid $main-button-color;\n    border-radius: 5px;\n    font-size: 14px;\n    font-weight: 700;\n    color: #fff;\n    text-decoration: none;\n    transition: 0.5s;\n  \n    &:hover {\n      color: $main-button-color;\n      background-color: #fff;\n      transition: 0.7s;\n    }\n  }\n}\n","styles\\blocks\\stars.scss":".stars {\n  display: flex;\n\n  &__star {\n    width: 15px;\n    height: 15px;\n    background-image: url(/images/star-active.svg);\n    background-repeat: no-repeat;\n    background-position: center;\n    margin-right: 4px;\n\n    &:last-child {\n      background-image: url(/images/star.svg);\n    }\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"bc29929db4a12614573734af207d3140","cacheData":{"env":{}}}