     position: absolute;\n      right: 0;\n      left: 0;\n      bottom: 0;\n      content: \"\";\n      height: 4px;\n      border-radius: 8px;\n      background-color: hsl(193, 100%, 43%);\n    }\n  }\n}\n","styles\\utilits\\_variables.scss":"$main-button-color: #00acdc;\n$card-width: 200px;\n$card-gap: 48px;\n$columns: 4;\n$fourColumns: catalogWigth($card-width, $card-gap, $columns);\n","styles\\blocks\\container.scss":".container {\n  display: flex;\n  justify-content: center;\n}\n","styles\\blocks\\content.scss":".content {\n  display: flex;\n  flex-wrap: wrap;\n  justify-content: space-between;\n  width: catalogWigth($card-width, $card-gap, $columns);\n  // gap: 50px 0;\n\n  &--margins {\n    margin: 0 40px;\n  }\n}\n","styles\\utilits\\_function.scss":"\n@function catalogWigth($card-width, $card-gap, $columns) {\n  @return $card-width * $columns + ($columns - 1) * $card-gap;\n}\n","styles\\blocks\\card.scss":".card {\n  box-sizing: border-box;\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  max-width: $card-width;\n  margin-top: 50px;\n  padding: 14px;\n  border: 1px solid #f3f3f3;\n  border-radius: 5px;\n  perspective: 100px;\n\n  &__image {\n    width: 160px;\n    height: 134px;\n    margin-top: 15px;\n    margin-bottom: 45px;\n    align-self: center;\n    transition: transform 400ms;\n  }\n\n  &__image:hover {\n    box-sizing: border-box;\n    transform: translate3D(0, 10px, 20px);\n    border: 3px solid #f3f3f3;\n  }\n  \n  &__button-To-Buy {\n    box-sizing: border-box;\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    height: 40px;\n    background-color: $main-button-color;\n    border: 1px solid $main-button-color;\n    border-radius: 5px;\n    font-size: 14px;\n    font-weight: 700;\n    color: #fff;\n    text-decoration: none;\n    transition-duration: 0.8s;\n  \n    &:hover {\n      color: $main-button-color;\n      background-color: #fff;\n      transition-duration: 0.6s;\n    }\n  }\n}\n","styles\\blocks\\price.scss":"\n.price {\n  display: flex;\n  justify-content: space-between;\n  align-items: baseline;\n  margin-bottom: 20px;\n\n  &__value {\n    font-size: 16px;\n    font-weight: 700;\n  }\n}\n","styles\\blocks\\rate.scss":"\n.rate {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  margin-bottom: 20px;\n}\n","styles\\blocks\\stars.scss":".stars {\n  display: flex;\n\n  &__star {\n    width: 15px;\n    height: 15px;\n    background-image: url(/images/star-active.svg);\n    background-repeat: no-repeat;\n    background-position: center;\n    margin-right: 4px;\n\n    &:last-child {\n      background-image: url(/images/star.svg);\n    }\n  }\n}\n","styles\\blocks\\product.scss":"\n.product {\n  \n  &__name {\n    font-weight: 500;\n    line-height: 15px;\n    font-size: 12px;\n    margin-bottom: 6px;\n  }\n  \n  &__code {\n    margin-bottom: 15px;\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"1b4519c50a8a61a3ee06064d5c5a1c53","cacheData":{"env":{}}}